{
  "name": "aofuji-analytics",
  "version": "0.1.4",
  "description": "Minimal alternative to Google Analytics.",
  "author": "DSRKafuU <dsrkafuu@outlook.com> (https://dsrkafuu.su)",
  "license": "MIT",
  "scripts": {
    "postinstall": "husky install",
    "vercel-build": "npm run build",
    "lint:web": "vue-cli-service lint --no-fix",
    "lint:api": "eslint api",
    "lint": "concurrently -m 1 npm:lint:web npm:lint:api",
    "dev:web": "vue-cli-service serve",
    "dev:api": "nodemon --watch src --watch api --watch utils api/index.js",
    "dev": "cross-env NODE_ENV=development env-cmd -f .env.development concurrently npm:dev:api npm:dev:web",
    "build:web": "vue-cli-service build",
    "build:api": "node utils/build.js",
    "build": "cross-env NODE_ENV=production env-cmd -f .env.production concurrently -m 1 npm:lint:web npm:lint:api npm:build:web npm:build:api",
    "start": "cross-env NODE_EsNV=production env-cmd -f .env.production node api/index.js"
  },
  "dependencies": {
    "aofuji-tracker": "~0.1.7",
    "axios": "~0.21.1",
    "bcryptjs": "~2.4.3",
    "bowser": "~2.11.0",
    "chart.js": "3.0.0-beta.11",
    "chartjs-chart-geo": "3.0.0-beta.9",
    "compression": "~1.7.4",
    "connect-history-api-fallback": "~1.6.0",
    "cookie-parser": "~1.4.5",
    "core-js": "~3.12.1",
    "cors": "~2.8.5",
    "dayjs": "~1.10.4",
    "express": "5.0.0-alpha.8",
    "is-localhost-ip": "~1.4.0",
    "isbot": "~3.0.26",
    "js-cookie": "~2.2.1",
    "jsonwebtoken": "~8.5.1",
    "lodash": "~4.17.21",
    "maxmind": "~4.3.1",
    "mongoose": "~5.12.11",
    "normalize.css": "~8.0.1",
    "on-headers": "~1.0.2",
    "serverless-http": "~2.7.0",
    "vue": "~2.6.12",
    "vue-router": "~3.5.1",
    "vuex": "~3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/eslint-parser": "^7.14.3",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@vue/cli-plugin-babel": "5.0.0-beta.1",
    "@vue/cli-plugin-eslint": "5.0.0-beta.1",
    "@vue/cli-plugin-router": "5.0.0-beta.1",
    "@vue/cli-plugin-vuex": "5.0.0-beta.1",
    "@vue/cli-service": "5.0.0-beta.1",
