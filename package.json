{
  "name": "aofuji-analytics",
  "version": "0.1.4",
  "description": "Minimal alternative to Google Analytics.",
  "author": "DSRKafuU <dsrkafuu@outlook.com> (https://dsrkafuu.su)",
  "license": "MIT",
  "scripts": {
    "postinstall": "husky install",
    "vercel-build": "npm run build",
    "lint:web": "eslint --ext .js,.jsx,.vue src",
    "lint:api": "eslint --ext .js api",
    "lint": "cross-env NODE_ENV=production env-cmd -f .env.production --silent concurrently -m 1 npm:lint:api npm:lint:web",
    "dev:web": "vite",
    "dev:api": "nodemon --watch src --watch api --watch utils api/index.js",
    "dev": "cross-env NODE_ENV=development env-cmd -f .env.development concurrently npm:dev:api npm:dev:web",
    "build:web": "vite build",
    "build:api": "node utils/build.js",
    "build": "cross-env NODE_ENV=production env-cmd -f .env.production --silent concurrently -m 1 npm:lint:web npm:lint:api npm:build:web npm:build:api",
    "start": "cross-env NODE_EsNV=production env-cmd -f .env.production --silent node api/index.js"
  },
  "dependencies": {
    "@vue/compat": "3.1.1",
    "aofuji-tracker": "~0.1.7",
    "axios": "~0.21.1",
    "bcryptjs": "~2.4.3",
    "bowser": "~2.11.0",
    "chart.js": "3.0.0-beta.11",
    "chartjs-chart-geo": "3.0.0-beta.9",
    "compression": "~1.7.4",
    "connect-history-api-fallback": "~1.6.0",
    "cookie-parser": "~1.4.5",
    "core-js": "~3.14.0",
    "cors": "~2.8.5",
    "dayjs": "~1.10.5",
    "express": "5.0.0-alpha.8",
    "is-localhost-ip": "~1.4.0",
    "isbot": "~3.0.26",
    "js-cookie": "~2.2.1",
    "jsonwebtoken": "~8.5.1",
    "lodash": "~4.17.21",
    "maxmind": "~4.3.2",
    "mongoose": "~5.12.13",
    "normalize.css": "~8.0.1",
    "on-headers": "~1.0.2",
    "serverless-http": "~2.7.0",
    "vue": "3.1.1",
    "vue-router": "~3.5.1",
    "vuex": "~4.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "3.1.1",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "esbuild": "^0.12.8",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^7.11.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.1",
    "sass": "^1.34.1",
    "sass-loader": "^12.1.0",
    "tar": "^6.1.0",
    "vite": "^2.3.7",
    "vite-plugin-svg-icons": "^0.7.0"
  }
